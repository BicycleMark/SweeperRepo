        Title="LoggingOptions" Height="300" Width="456.667">
    <Grid Background="{DynamicResource DIAG_BACKGROUND}" Margin="0,0,-172,13.667">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="380*"/>
            <ColumnDefinition Width="67*"/>
