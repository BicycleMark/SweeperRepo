        <CheckBox Content="Mouse Messages" HorizontalAlignment="Left" Margin="41,69,0,0" VerticalAlignment="Top" FontSize="16" Foreground="{DynamicResource DIAG_FOREGROUND}" Effect="{DynamicResource DIAGTEXTEFFECT}"/>
        <CheckBox Content="Game Messages" HorizontalAlignment="Left" Margin="339,72,0,0" VerticalAlignment="Top" FontSize="16" Grid.ColumnSpan="2" Foreground="{DynamicResource DIAG_FOREGROUND}" Effect="{DynamicResource DIAGTEXTEFFECT}"/>
        <ListBox HorizontalAlignment="Left" Height="158" Margin="41,93,0,0" VerticalAlignment="Top" Width="193" Effect="{DynamicResource DIAGTEXTEFFECT}"/>
        <ListBox HorizontalAlignment="Left" Height="158" Margin="339,96,-85,0" VerticalAlignment="Top" Width="193" Grid.ColumnSpan="2" Effect="{DynamicResource DIAGTEXTEFFECT}"/>

    </Grid>
</Window>
